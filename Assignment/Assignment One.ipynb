    "#gpa point to grade conversion\n",
    "\n",
    "gpa = 3.99\n",
    "\n",
    "if (gpa == 5):\n",
    "    print ('A+')\n",
    "elif (gpa >= 4):\n",
    "    print ('A')\n",
    "elif (gpa >= 3.5):\n",
    "    print ('A-')\n",
    "elif (gpa >= 3):\n",
    "    print ('B')\n",
    "elif (gpa >= 2):\n",
    "    print ('C')\n",
    "elif (gpa >= 1):\n",
    "    print ('D')\n",
    "else:\n",
    "    print ('F')"
